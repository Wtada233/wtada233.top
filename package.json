{
  "name": "blog",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm run validate-config && astro dev",
    "start": "pnpm run validate-config && astro dev",
    "check": "astro check",
    "validate-config": "tsx scripts/validate-config.ts",
    "build": "pnpm run validate-config && tsx scripts/staticize-github-cards.ts && tsx scripts/localize-external-images.ts && astro build && tsx scripts/generate-og-images.ts && tsx scripts/font-subset.ts && pnpm run check-integrity",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "tsc --noEmit --isolatedDeclarations",
    "new-post": "tsx scripts/new-post.ts",
    "check-integrity": "tsx scripts/check-integrity.ts",
    "format": "biome format --write ./src ./scripts",
    "lint": "biome check --write ./src ./scripts",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^6.0.2",
    "@expressive-code/core": "^0.41.5",
    "@expressive-code/plugin-collapsible-sections": "^0.41.5",
    "@expressive-code/plugin-line-numbers": "^0.41.5",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@swup/astro": "^1.7.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/blueimp-md5": "^2.18.2",
    "@vite-pwa/astro": "^1.2.0",
    "astro": "5.16.6",
    "astro-expressive-code": "^0.41.5",
    "blueimp-md5": "^2.19.0",
    "flexsearch": "^0.8.212",
    "mdast-util-to-string": "^4.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-components": "^0.3.0",
    "rehype-katex": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-directive-rehype": "^0.4.2",
    "remark-github-admonitions-to-directives": "^2.1.0",
    "remark-math": "^6.0.0",
    "remark-sectionize": "^2.1.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.6",
    "@biomejs/biome": "2.3.10",
    "@rollup/plugin-yaml": "^4.1.2",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "cheerio": "^1.1.2",
    "marked": "^17.0.1",
    "mdast-util-directive": "^3.1.0",
    "mdast-util-from-markdown": "^2.0.2",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "remark-directive": "^4.0.0",
    "remark-parse": "^11.0.0",
    "subset-font": "^2.4.0",
    "tsx": "^4.21.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.25.76"
  },
  "packageManager": "pnpm@10.26.2"
}
